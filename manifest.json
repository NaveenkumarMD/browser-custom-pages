{
    "name": "Extension manager",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "Src/background.js"
    },
    "action":{
        "default_popup": "Src/popup.html"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "management",
        "tabs"
    ]
}